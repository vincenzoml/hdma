	Keywords = {LAMBDA-CALCULUS / PI-CALCULUS / TYPES / REDUCTION STRATEGIES / BEHAVIOURAL EQUIVALENCES / FULL ABSTRACTION},
	Language = {English},
	Month = Aug,
	Number = {RR-3470},
